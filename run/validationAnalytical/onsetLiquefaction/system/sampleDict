FoamFile
{
    version         2.0;
    format          ascii;
    class           dictionary;
    location        "system";
    object          sampleDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

interpolationScheme cellPointFace;

setFormat raw;

sets
(
    Profile2
    {
        type            uniform;
        axis            z;
        start           (87 0 0);
        end             (87 0 -1);
        nPoints	 1000;
    }


);


fields
(
    liqueCriteria
);


