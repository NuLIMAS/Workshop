FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    U
    {

        solver          PCG;
        preconditioner  DILU;        
        tolerance       1e-15;
        relTol          0;
        smoother        GaussSeidel;
        cacheAgglomeration true;
        nCellsInCoarsestLevel 20;
        agglomerator    faceAreaPair;
        mergeLevels     1;
        minIter         1;
       
    }


}

stressAnalysis
{
    compactNormalStress yes;
    nCorrectors     1500;
    U               9e-04;
    
}
relaxationFactors
{
    fields
    {
        U    0.2;
    }

}


