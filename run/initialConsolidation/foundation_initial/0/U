FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    
    seaBed
    {
        type            tractionDisplacement;
        traction        uniform (0 0 0);
        pressure        uniform -4905;
        //pressure        uniform 0;
        //type            fixedValue;
        value           uniform (0 0 0);
    }

    seabedFoundation
    {
        type            tractionDisplacement;
        traction        uniform (0 0 0);
        pressure        uniform -5589.25;
        //pressure        uniform -4905;
        //pressure        uniform -273;
        value           uniform (0 0 0);
    }
    
    left
    {
        //type            slip;
        //type            zeroGradient;
        //type            fixedValue;
        //value           uniform (0 0 0);
        type splitCyclic;
        neighbourPatch left;
    } 

    right
    {
        //type            slip;
        //type            zeroGradient;
        //type            fixedValue;
        //value           uniform (0 0 0);
        type splitCyclic;
        neighbourPatch left;
    }
    
    soilDomainBottom
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    frontBack
    {
        type            empty;
    }

}

