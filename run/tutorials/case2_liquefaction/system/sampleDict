FoamFile
{
    version         2.0;
    format          ascii;
    class           dictionary;
    location        "system";
    object          sampleDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

interpolationScheme cellPointFace;

setFormat raw;

sets
(
    Profile1
    {
        type        uniform;
        axis        z;
        start       (1.59 0 0);
        end         (1.59 0 -0.4);
        nPoints     100;
    }


);


fields
(
    p
    liqueCriteria
);


