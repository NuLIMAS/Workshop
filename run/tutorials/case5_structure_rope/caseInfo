y  2.056;
nxs 40;
nxf 20;
nzt 1;
nzb 23;
nz 10 ;
nzf 8;

/* Material properties */
E            1.5e6; // Elastic modulus
Ea           1e10; // Elasic modulus anchor
nu           0.31; // Poisson ratio
k            1.45e-5; // Permeability
K            1.9e9; // True Bulk Modulus
p0           44145; //  Atm pressure
Sr           1; // Degree of saturation
n1           0.5; // Porosity
n2           0.37; // Porosity of compacted soil
relDensity   0.25; // Realtive density
emax         1.159; // Max void ratioq
emin         0.586; // Min void ratio
rhos         2656; // Density of soil grains
rhof         1000; // Density of water
wh           0; // height of water above the seabed in numerical model
sg           2.656; // specific gravity
planeStress    no;
gamma        0.981e4; // Specific weight of water
/* Parameters to initialize liquefaction or run in steady state*/
steady         no;
Cl             0;
ropeForce      1000;


/* Pressure buildup properties */
gammaD       8120.0; // Sp weight of the soil
k0           0.45; // Coeff of lateral stress



/* Foundation parameters */ 
QbyBL        2543.53;//  (rhoa-rhof)*g*h
rhoa         1560; // Desity of foundation
Ur1           (0 0 1000);//rope forces
Ur2           (0 0 1000);//rope forces
fw            2.056; //foundation width
rhodg         2629.8; //gamma' - (rhoa-rhof)*g

/*Post processing*/
probes       (($xm 0 -0.12)  
    	      ($xm 0 -0.22)   
   	      ($xm 0 -0.4)  
  	      ($xm 0 -0.6) 
  	      ($xm 0 -0.85)
  	      (4.58 0 -0.12)  
