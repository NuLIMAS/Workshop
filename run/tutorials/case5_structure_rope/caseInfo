/* Wave properties*/
waveHeight  0.5; // Wave height in meters
waterDepth  4.5; // Wave height in meters
period      2.69; // Wave period in seconds
waveLength  11.16; // Wave length in meters
phase       -0.25; 
offset      0;

/* Mesh parametrs and probes */

xm 5.58; //Location of probes middle of the domain
z1 0;    //Seabed mudline
z2 -1;//Impermeable base
z3 -0.05;//Impermeable base
z4 0.463;
x1 0;    // Seabed start
