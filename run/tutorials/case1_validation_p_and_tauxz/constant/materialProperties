FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      materialProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
#include "$FOAM_CASE/caseInfo"

E              E [ 1 -1 -2 0 0 0 0 ] $E; // Elastic modulus

nu             nu [ 0 0 0 0 0 0 0 ] $nu; //Poisson ratio

k              k [ 0 1 -1 0 0 0 0 ] $k; // Permeability

K              K [ 1 -1 -2 0 0 0 0 ] $K; // True Bulk modulus

Sr             Sr [ 0 0 0 0 0 0 0 ] $Sr; // Degree of saturation

p0             p0 [ 1 -1 -2 0 0 0 0 ] $p0; // Pressure at rest

gamma          gamma [ 1 -2 -2 0 0 0 0 ] $gamma; // Specific weight of water

n              n [ 0 0 0 0 0 0 0 ] $n1; // Porosity 

planeStress     no;

